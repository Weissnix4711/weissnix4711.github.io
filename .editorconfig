
# editorconfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{html,htm,markdown,md}]
charset = utf-8
indent_size = 2
indent_style = space
trim_trailing_whitespace = true

[*.{yml,yaml}]
charset =  = utf-8
indent_size = 2
indent_style = space
trim_trailing_whitespace = true
